{
  "name": "elibrary",
  "description": "An all-in-one cpp library",
  "version": "0.20.2",
  "license": "Apache-2.0",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "default-features": ["module-multimedia"],
  "features": {
    "module-extension-geoip": {
      "description": "eLibrary::Extension::GeoIP",
      "dependencies": ["libmaxminddb"]
    },
    "module-multimedia": {
      "description": "eLibrary::Multimedia",
      "dependencies": ["ffmpeg", "openal-soft"]
    }
  }
}