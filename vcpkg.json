{
  "name": "elibrary",
  "description": "An all-in-one cpp library",
  "license": "Apache-2.0",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ],
  "version-string": "2023.15",
  "default-features": ["module-multimedia"],
  "features": {
    "feature-archive": {
      "description": "eLibrary::IO::Archive",
      "dependencies": ["libarchive"]
    },
    "module-multimedia": {
      "description": "eLibrary::Multimedia",
      "dependencies": ["ffmpeg", "openal-soft"]
    }
  }
}